{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "react-hooks/exhaustive-deps": "warn",
    "@next/next/no-html-link-for-pages": "warn",
    "prefer-const": "warn",
    "react/no-unescaped-entities": "error",
    "no-var": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/", "build/", "dist/"]
}
